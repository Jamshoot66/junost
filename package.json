{
  "name": "junost",
  "version": "1.0.1",
  "description": "Junost project made by ANMEDIO",
  "main": "src/@depricated/gulpfile.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --hot --inline --host localhost --port 8080",
    "static-build": "cross-env NODE_ENV=development gulp build",
    "react-build": "cross-env NODE_ENV=production webpack --mode=production",
    "react-dev-build": "cross-env NODE_ENV=production webpack --mode=development",
    "build": "npm run static-build && npm run react-build",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "lint-fix": "eslint src -c .eslintrc --ext js,jsx --fix",
    "start:next": "NODE_PATH=src/ next"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/anmedio/grades.git"
  },
  "keywords": [
    "grades",
    "digital",
    "junost",
    "anmedio"
  ],
  "author": "Artyom Ivanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anmedio/grades/issues"
  },
  "homepage": "https://gitlab.com/anmedio/grades#readme",
